    - /bin/bash
    - ./tmp/init-dataBase.sh
    depends_on:
      sqlserver:
        condition: service_started
    image: mcr.microsoft.com/mssql-tools:latest
    links:
    - sqlserver
    networks:
      default: null
    volumes:
    - type: bind
      source: C:\Desenvolvimento\gamesManagement\01-create-database.sql
      target: /tmp/01-create-database.sql
      bind:
        create_host_path: true
    - type: bind
      source: C:\Desenvolvimento\gamesManagement\02-create-tables.sql
      target: /tmp/02-create-tables.sql
      bind:
        create_host_path: true
    - type: bind
      source: C:\Desenvolvimento\gamesManagement\03-insert-data.sql
      target: /tmp/03-insert-data.sql
      bind:
        create_host_path: true
    - type: bind
      source: C:\Desenvolvimento\gamesManagement\init-dataBase.sh
      target: /tmp/init-dataBase.sh
      bind:
        create_host_path: true
