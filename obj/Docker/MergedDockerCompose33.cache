      MSSQL_SA_PASSWORD: 1q2w3e4r@#$$
      SA_PASSWORD: 1q2w3e4r@#$$
      sa: 1q2w3e4r@#$$
    hostname: sqlserver
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
